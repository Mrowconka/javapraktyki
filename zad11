package olgamrowca;

public class zadanie11 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double ankietowani = 12467;
		double energetyk = ankietowani * 0.14;
		double cytrusowy = energetyk* 0.64;
		
		System.out.println("Liczba osób które kupują przynajmniuej jeden energetyk ntygodniowo wynosi :" + energetyk);
		System.out.println("Liczba osób które preferują napój energetyczny o smaku cytrtusowy wynosi :" + cytrusowy);
		

	}

}
