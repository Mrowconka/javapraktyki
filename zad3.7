package olga;
import java.io.*;
import java.util.Scanner;

public class zadanie3 {

	public static void main(String[] args) throws IOException{
		Scanner input= new Scanner(System.in);
		PrintWriter plik = new PrintWriter("dane.txt");
		
		int godziny;
		int szybkosc;
		System.out.println("Podaj szybkość pojazdu (w kilometrach na godzinę): ");
		szybkosc=input.nextInt();
		System.out.println("Podaj liczbę godzin podróży: ");
		godziny=input.nextInt();
		

		
		if(godziny < 1 && szybkosc < 1) 
		{
			System.out.println("Podano złe wartości!");
		} 
		
		else 
		
		{
		
			for(int i = 1; i <= godziny; i++) {
				double odleglosc = godziny * szybkosc;
				plik.println("Przebyta odległość w " + i + "godzinie: " + odleglosc*i/godziny);
			}
		}
		plik.close();
	}	

}
